function draw_game()
	cls(0)
	starfield()
	local eshoot
	
	-- draw unicorn
	spr(uni.spr,uni.x,uni.y,2,2)
	
	-- draw fart
	//draw_fart()
	
	-- draw bullets
	for bullet in all(bullets) do
	 draw_spr(bullet)
	end
	
	for ebullet in all(ebullets) do
	 draw_spr(ebullet)
	end
	
	
	-- draw enemies
	for enemy in all(enemies) do
	 draw_spr(enemy)
	 spr(enemy.fspr,
	 				enemy.x,
	 				enemy.y-8)
	 if pause_timer >=80 then
		 eshoot = rnd(100)
			if eshoot <= 0.7 then //set to 50 for accurate american police experience
				if enemy.y <= 64 then
					load_ebullet(enemy)
				end
			end		
		end		
	end
	
	-- muzzle flash
	if muzzle>0 then
		if uni.dir == "left" then
			circfill(uni.x - 3,
												uni.y-2,
												muzzle,
												7)
		end
		if uni.dir == "right" then
			circfill(uni.x+19,
												uni.y-2,
												muzzle,
												7)
		end
		if uni.dir == "foreward" then
			circfill(uni.x+7,
												uni.y-2,
												muzzle,
												7)
		end
	end
	
	-- 999999 is the biggest number
	print("score: ",45,3,12)
	print(pad(""..score2),70,3,12)
	print(pad(""..score1),82,3,12)
	
	-- draw hearts
	for i=1,total_lives do
		if cur_lives>=i then
			spr(13,i*9-8,2)
		else
			spr(14,i*9-8,2)
		end
	end
end

