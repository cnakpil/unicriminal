----------- controls -----------

function controls()
	if btnp(0) then
	 sidestep("left",-4)
	end
	if btnp(1) then
		sidestep("right",4)
	end
	if btnp(5) then
	 load_bullet()
		sfx(0)
		muzzle=4
	end
	if btnp(4) then
		mode="over"
		music(5,5,1)
	end
end

function sidestep(dir,spd)
		uni.spdx=spd
 	if uni.animindex>2 then
	 	uni.animindex=1
	 end
	 uni.spr=anims[dir][uni.animindex]
	 uni.animindex+=1
	 timer=6
	 uni.dir = dir
end

function reset_anim()
	if timer>0 then
		uniframe=0
		timer-=1
	end
	if timer==0 then
		uni.dir = "foreward"
		uniframe+=1
		if uni.animindex>2 then
	 	uni.animindex=1
		end
		if uniframe%3==0 then
			uni.spr=anims["default"][uni.animindex]
			uni.animindex+=1 
		end
	end
end

function load_ebullet(enemy)
 local new_bullet={}

	new_bullet.x=enemy.x
	new_bullet.y=enemy.y
	
	new_bullet.spd=-3
	new_bullet.spr=71
	new_bullet.colw=8
	new_bullet.colh=8
	new_bullet.dmg=1
	new_bullet.sy=4
	add(ebullets,new_bullet)
end

function load_bullet()
 local new_bullet={}
 if uni.dir == "left" then
 	new_bullet.x=uni.x - 5
		new_bullet.y=uni.y-3.5
 end
 if uni.dir == "right" then
 	new_bullet.x=uni.x + 17
		new_bullet.y=uni.y-3.5
 end
 if uni.dir == "foreward" then
 	new_bullet.x=uni.x+3
		new_bullet.y=uni.y-3.5
 end
	
	new_bullet.spd=5
	new_bullet.spr=065
	new_bullet.colw=8
	new_bullet.colh=8
	new_bullet.dmg=1
	new_bullet.sy=-4
	add(bullets,new_bullet)
end

function set_bounds()
	if uni.x>112 then
		uni.x=112
	end
	if uni.x<0 then
		uni.x=0
	end
end
