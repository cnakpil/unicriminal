------------ update ------------

function update_game()
	
	-- reset unicorn state
	uni_reset()
	
	-- controls
	controls()
	
	-- move unicorn
	uni_move()
	
	-- move+animate unicorn farts
	unifart_anim()
	
	-- animate muzzle flash
	muzzle_flash()
	
	-- move+animate bullet
	bullet_anim()
	
	-- enemy animations
	espawn_anim()
	enemy_anim()
	if pause_timer <= 80 then
		pause_timer += 1
	end
	
	if spawn_enemy_timer <= 0 then
		update_difficulty()
		spawn_enemies()
		spawn_enemy_timer = 140
	else
		spawn_enemy_timer -= 1
	end
	
	ebullet_anim()
	
	-- animate enemy flames
	flame_anim(enemies,21,24)
	
	-- set screen bounds
	set_bounds()
	
	-- collisions
	shoot_enemy()
	uni_crash()
	shoot_unicorn()
	
	-- animate stars
	animate_stars()
	
	-- clean up off screen enemies
	clean()
	
end

function update_start()
	if btnp(5) or btnp(4) then
		start_game()
		music(0,10,1)
		pause_timer = 0
	end
end

function update_over()
	if btnp(5) or btnp(4) then
		mode="start"
	end
end
