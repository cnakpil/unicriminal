-------- misc functions --------

function starfield()
	for i=1,#stars do
	 local cur_star=stars[i]
	 
	 if cur_star.spd<1 then
	 	cur_star.color=1
	 	elseif cur_star.spd<1.5 then
			cur_star.color=13
		end
		pset(cur_star.x,
							cur_star.y,
							cur_star.color)
	end
end

function animate_stars()
	for i=1,#stars do
		local cur_star=stars[i]
		cur_star.y+=cur_star.spd
		if cur_star.y>128 then
		 cur_star.y-=128
		end
	end
end

function blink()
	local b_anim={8,8,8,8,8,8,8,8,
															8,8,8,8,8,8,8,8,
															9,9,9,9,9,9,9,9,
															9,9,9,9,9,9,9,9,
															11,11,11,11,11,11,
															11,11,11,11,11,
															11,11,11,11,11,
															12,12,12,12,12,12,
															12,12,12,12,12,
															12,12,12,12,12,
															13,13,13,13,13,13,
															13,13,13,13,13,
															13,13,13,13,13}
	if blink_count>=#b_anim then
		blink_count=1
	end
	return blink_count
end

function spawn_enemies()
	local espr
	local efspr
	local hp
	local edang
	local etype
	
	
	for j = 0,enemyrows do
		for i=0,7 do
			edang = randi(100)
		 if edang>=danger_level then
		  espr=18
		  efspr=21
		  hp=1
		  etype = 1
		 else
		  espr=34
		  efspr=37
		  hp=2
		  etype = 2
		 end
 
		 local new_enemy={}
			--randomize location
			new_enemy.x=14+(i*14)
			new_enemy.y=-8 - ((enemyrows-j)*15)
			new_enemy.spr=espr
			new_enemy.fspr=efspr
			new_enemy.ghost=false
			new_enemy.colw=8
			new_enemy.colh=8
			new_enemy.hp=hp
			new_enemy.wave = j
			new_enemy.etype = etype
			add(enemies,new_enemy)
		end
	end
end

-- delete any enemies off screen --
function clean()
 for i=#enemies,1,-1 do
		--move
	 local cur_enemy=enemies[i]
	 
	 if cur_enemy.y>128 then
	 	del(enemies,cur_enemy)
	 end
	end
end

-- hacky way to add padding to strings--
-- trying to do recursion made a mem leak--
function pad(string)
		if #string ==3 then return string
		elseif #string ==2 then return "0"..string
		elseif #string ==1 then return "00"..string
		elseif #string == 0 then return "000"
	end
end

-- add points to score
function update_score(points)
	score1 += points
	if score1 >= 999 then
		score1 -= 999
		score2 += 1
	end
end

-- adjust difficulty of game
-- based on score
function update_difficulty()
	if score1 >= 50 and enemyrows < 5 then
			enemyrows += 1
	end
	if danger_level <100 then
		if flr(score1/30) > level  then
			danger_level += 5
			level +=1
		end
	end
end

-- return a random integer between 0 and h
function randi(h)
	return flr(rnd(h))
end
