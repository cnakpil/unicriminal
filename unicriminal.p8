function ebullet_anim()
 for i=#ebullets,1,-1 do
		--move
	 local cur_bullet=ebullets[i]
	 cur_bullet.y-=cur_bullet.spd
	 
	 if cur_bullet.y>128 then
	 	del(ebullets,cur_bullet)
	 end
	 
	 --animate
	 cur_bullet.spr+=1
		if cur_bullet.spr>72 then
			cur_bullet.spr=71
		end	 
	end
end

function bullet_anim()
 for i=#bullets,1,-1 do
		--move
	 local cur_bullet=bullets[i]
	 cur_bullet.y-=cur_bullet.spd
	 
	 if cur_bullet.y<-8 then
	 	del(bullets,cur_bullet)
	 end
	 
	 --animate
	 cur_bullet.spr+=1
		if cur_bullet.spr>68 then
			cur_bullet.spr=66
		end	 
	end
end

