anims={
 default={fr=5,128,130},
 left={fr=5,132,134},
 right={fr=5,136,138},
 fright={fr=5,164,162}
}

timer=0
uniframe=0
emove_anim=0

function _init()
	-- clear screen
	cls(0)
	
	music(5,10,1)
	mode="start"
	blink_count=0
end

function _update()
	blink_count+=1

	if mode=="game" then
 	update_game()
 elseif mode=="start" then
  update_start()
 elseif mode=="over" then
 	update_over()
 end
end

function _draw()
 if mode=="game" then
 	draw_game()
 elseif mode=="start" then
 	draw_start()
 elseif mode=="over" then
  draw_over()
 end
end

function start_game()
	-- basic variables
	mode="game"
	muzzle=0
	score1=0
	score2=0	
	total_lives=4
	cur_lives=4
	danger_level = 0 //between 0 and 100
	level = 0
	enemyrows = 1
	spawn_enemy_timer=200
	
	-- unicorn object
	uni={}
	uni.spr=128
	uni.x=60
	uni.y=100
	uni.spdx=0
	uni.animindex=2
	uni.ghost=false
	uni.colw=16
	uni.colh=16
	uni.dir = "forward"
	
	-- star object array
	stars={}
	for i=1,100 do
		local new_star={}
		new_star.x=flr(rnd(128))
		new_star.y=flr(rnd(128))
		new_star.spd=rnd(1.5)+0.5
		new_star.color=6
		add(stars,new_star)		
	end
	
	-- bullet object array
	bullets={}
	
	-- enemy bullet object array
	ebullets={}
	
	-- enemy object array
 enemies={}
 spawn_enemies()
	
	-- unicorn fart array
	farts={}
	
end
